{
    "name": "maxfactor/support",
    "description": "Re-hosted:fastfwd, Support and helpers for Laravel PHP based projects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Mark",
            "email": "marco.mark@dewsign.co.uk",
            "homepage": "https://dewsign.co.uk"
        }
    ],
    "repositories": [
        {
            "name": "spatie/laravel-view-components",
            "type": "vcs",
            "url": "https://github.com/fastfwd/laravel-view-components.git"
        }
    ],
    "require": {
        "laravel/framework": ">=5.5",
        "symfony/intl": "^4.0",
        "dereuromark/media-embed": "^0.4.1",
        "spatie/laravel-view-components": "dev-master",
        "laravelium/sitemap": "^2.0|^3.0|^6.0|^7.0|^8.0",
        "dannyvankooten/laravel-vat": "^2.0.4"
    },
    "autoload": {
        "psr-4": {
            "Maxfactor\\Support\\": "src"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3"
    },
    "extra":{
        "laravel": {
            "providers": [
                "Maxfactor\\Support\\MaxfactorServiceProvider"
            ],
            "aliases": {
                "Maxfactor": "Maxfactor\\Support\\Facades\\Maxfactor",
                "Format": "Maxfactor\\Support\\Facades\\Format"
            }
        }
    }
}
